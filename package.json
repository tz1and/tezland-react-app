    "start": "vite --mode development --port 3006 --cors true",
    "start:staging": "vite --mode staging --port 3006 --cors true",
    "build": "vite build --mode production --sourcemap false",
    "build:dev": "NODE_OPTIONS=--max-old-space-size=32768 vite build --mode development --sourcemap true",
    "build:staging": "NODE_OPTIONS=--max-old-space-size=32768 vite build --mode staging --sourcemap true",
    "preview": "vite preview --port 3006",
    "test": "vitest --mode development",
    "analyze": "source-map-explorer 'dist/static/js/*.js'",
    "generate-licenses": "node scripts/generate-licenses.js",
    "generate-gql-schema": "graphql-codegen && get-graphql-schema -h 'X-API-KEY=changeme' http://localhost:42000/v1/graphql > src/graphql/generated/schema.graphql"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
